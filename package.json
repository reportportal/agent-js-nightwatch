{
  "name": "@reportportal/agent-js-nightwatch",
  "version": "0.2.0",
  "description": "Agent for integration Nightwatch with ReportPortal.",
  "main": "build/index.js",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/reportportal/agent-js-nightwatch.git"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf ./build",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "npm run lint -- --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "example:chai": "npm run build && cd examples && npm run chai",
    "example:chromeDriver": "npm run build && cd examples && npm run chromeDriver",
    "example:chromeDriver:realTimeReporter": "npm run build && cd examples && npm run chromeDriver:realTimeReporter",
    "example:chromeDriver:realTimeReporter:parallelRun": "npm run build && cd examples && npm run chromeDriver:realTimeReporter:parallelRun"
  },
  "dependencies": {
    "moment": "^2.22.1",
    "node-ipc": "9.1.1",
    "reportportal-client": "^5.5.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/chromedriver": "^2.38.0",
    "@types/jest": "^25.2.1",
    "@types/nightwatch": "^1.1.3",
    "@types/node-ipc": "9.1.2",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "chai": "^4.1.2",
    "chromedriver": "81.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.3.0",
    "nightwatch": "1.3.4",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
  },
  "directories": {
    "src": "./src"
  },
  "engines": {
    "node": ">= 8.0.x"
  },
  "keywords": [
    "epam",
    "reports",
    "portal",
    "nightwatch",
    "reporters",
    "reportportal"
  ]
}
